{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "framework": null,
  "redirects": [
    {
      "source": "/tools/:slug",
      "destination": "/tool/:slug",
      "permanent": true
    },
    {
      "source": "/rec/:slug",
      "destination": "/tool/:slug",
      "permanent": true
    },
    {
      "source": "/newsletter/how-to-sell-ai-services",
      "destination": "/newsletter",
      "permanent": true
    },
    {
      "source": "/tool/jasper",
      "destination": "/tool/jasper-ai",
      "permanent": true
    },
    {
      "source": "/tool/copyai",
      "destination": "/tool/copy-ai",
      "permanent": true
    },
    {
      "source": "/subscribe",
      "destination": "/newsletter",
      "permanent": true
    },
    {
      "source": "/privacy",
      "destination": "/privacy-policy",
      "permanent": true
    },
    {
      "source": "/submit",
      "destination": "/tools",
      "permanent": true
    },
    {
      "source": "/categories",
      "destination": "/tools",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
